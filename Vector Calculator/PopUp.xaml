<Window x:Class="Vector_Calculator.PopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vector_Calculator"
        mc:Ignorable="d"
        Title="PopUp" Height="175" Width="300">
    <StackPanel>
        <TextBlock FontSize="25">Enter Scalar:</TextBlock>
        <TextBox Name="txtBox" FontSize="30" FontWeight="DemiBold" Height="50"></TextBox>
        <Button Name="ButtonEnter" Height="50">Enter</Button>
    </StackPanel>
</Window>
